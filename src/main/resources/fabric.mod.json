{
  "schemaVersion": 1,
  "license": "MIT",
  "id": "johnmod",
  "version": "${version}",

  "name": "John's Revival",
  "description": "This is a revival of of ImmortalHDFilms's John mod into Minecraft Java 1.19.3's Fabric Mod loader!",
  "authors": [
    "AquaticTyphoon!"
  ],
  "contributors": [],
  "contact": {
    "homepage": "https://github.com/AquaticTyphoon/johhmod-fabric",
    "sources": "https://github.com/AquaticTyphoon/johhmod-fabric/issues"
  },

  "license": "CC0-1.0",
  "icon": "assets/johnmod/icon.png",

  "environment": "*",
  "entrypoints": {
